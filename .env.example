# Environment variables for Moodmate application

# Flask secret key for session management
SECRET_KEY=your_random_secret_key_here

# JWT secret key for token generation
JWT_SECRET_KEY=your_random_jwt_secret_key_here

# MongoDB connection string
# For local development: mongodb://localhost:27017/
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/database_name
MONGODB_URI=mongodb://localhost:27017/

# Port (Render automatically sets this)
PORT=5000